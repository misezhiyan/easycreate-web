CREATE TABLE `field` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT '主键',
  `field_name` varchar(100) CHARACTER SET utf8mb4 DEFAULT NULL COMMENT '字段名',
  `field_comment` varchar(200) CHARACTER SET utf8mb4 DEFAULT NULL COMMENT '字段注释',
  `field_bz` varchar(500) CHARACTER SET utf8mb4 DEFAULT NULL COMMENT '字段备注',
  `field_type` varchar(50) CHARACTER SET utf8mb4 DEFAULT NULL COMMENT '字段类型',
  `field_length` int(10) DEFAULT NULL,
  `length_after_point` int(3) DEFAULT '0' COMMENT '小数位数',
  `is_business` int(1) DEFAULT NULL COMMENT '是否业务字段  0:否  1:是',
  `create_by` int(11) DEFAULT NULL,
  `create_date` datetime DEFAULT NULL,
  `update_by` int(11) DEFAULT NULL,
  `update_date` datetime DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=85 DEFAULT CHARSET=latin1